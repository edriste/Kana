; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Kana"
config/features=PackedStringArray("4.3", "C#", "Mobile")
run/max_fps=60
config/icon="res://icon.svg"
run/handheld/orientation=1

[autoload]

SoundController="*res://scenes/globals/soundcontroller/SoundController.tscn"
CardRepository="*res://scenes/globals/cardrepository/CardRepository.tscn"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=540
window/size/window_height_override=960
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[dotnet]

project/assembly_name="Kana"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
